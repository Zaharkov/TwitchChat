      },
      {
        "text": "божечки ты мой! Самый огромный болт, который я видел - {1} см...в диаметре Kappa",
        "access": [ "broadcaster" ]
      }
    ],
    "params": [
      { //0
        "type": "randomNumber",
        "min": 1,
        "max": 35
      },
      { //1
        "type": "randomNumber",
        "min": 40,
        "max": 50
      }
    ]
  },
  {
    "name": "Шейкер",
    "type": "write",
    "access": [ "default" ],
    "cooldownType": "hybrid",
    "cooldownTime": 10,
    "needPrefix": true,
    "texts": [
      {
        "text": "хей, патимейкер!",
        "access": [ "default" ]
      },
      {
        "text": "хэй, baby, брекеты, косичка, миниюбка, кепка!",
        "access": [ "default" ]
      },
      {
        "text": "этот трек не для тверка, да и похрен, дискотека!",
        "access": [ "default" ]
      },
      {
        "text": "хэй, DJ!",
        "access": [ "default" ]
      },
      {
        "text": "хэй, битмейкер!",
        "access": [ "default" ]
      },
      {
        "text": "хэй, уличный дэнсер!",
        "access": [ "default" ]
      },
      {
        "text": "до свидули",
